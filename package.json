{
  "name": "versatiles-documentation",
  "version": "1.0.0",
  "description": "Documentation for VersaTiles",
  "homepage": "https://github.com/versatiles-org/versatiles-documentation#readme",
  "bugs": {
    "url": "https://github.com/versatiles-org/versatiles-documentation/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/versatiles-org/versatiles-documentation.git"
  },
  "scripts": {
    "dev": "vitepress dev .",
    "build": "npm run build:screenshots && npm run build:vitepress",
    "build:vitepress": "vitepress build .",
    "build:screenshots": "tsx scripts/process-screenshots.ts",
    "preview": "vitepress preview .",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "upgrade": "npm-check-updates -u && rm -f package-lock.json; rm -rf node_modules; npm i && npm update"
  },
  "license": "Unlicense",
  "type": "commonjs",
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.3.0",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.8.0",
    "github-slugger": "^2.0.0",
    "js-yaml": "^4.1.1",
    "npm-check-updates": "^19.4.1",
    "prettier": "^3.8.1",
    "sharp": "^0.34.5",
    "tsx": "^4.21.0",
    "typescript-eslint": "^8.56.0",
    "vitepress": "^1.6.4",
    "vue-eslint-parser": "^10.4.0"
  }
}
